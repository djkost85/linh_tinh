		</group>	
		<group snapToCell="true" gridPosition="CR" cellPosition="CR" orientation="horizontal" spacing="0" paddingV="5" paddingH="-10">
			<scrubBar back="assets/skins/skin/scrubBack.png"  position="assets/skins/skin/position.png" dif="1" width="150" orientation="horizontal" />							
		</group>
		<group snapToCell="true" gridPosition="CR" cellPosition="CR" orientation="horizontal" spacing="13.4" paddingH="-10" paddingV="20">
			<button out="assets/skins/skin/left_face.png" over="assets/skins/skin/left_face_over.png" action="left_face" tooltip="" />
			<button out="assets/skins/skin/right_face.png" over="assets/skins/skin/right_face_over.png" action="right_face" tooltip="" />
			<button out="assets/skins/skin/front_face.png" over="assets/skins/skin/front_face_over.png" action="front_face" tooltip="" />
			<button out="assets/skins/skin/back_face.png" over="assets/skins/skin/back_face_over.png" action="back_face" tooltip="" />	
		</group>
	</groups>
