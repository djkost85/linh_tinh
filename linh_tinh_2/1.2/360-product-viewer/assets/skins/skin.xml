<?xml version="1.0" encoding="utf-8" ?>
<data>
	<groups>
		<group snapToCell="true" gridPosition="CR" cellPosition="CR" orientation="horizontal" paddingV="0" paddingH="0">
			<backgroundButton backColor="0xCCCCCC" backAlpha=".7" cornerRadius="10" width="169" height="75"/>
		</group>
		<group snapToCell="true" gridPosition="CR" cellPosition="CR" orientation="horizontal" spacing="2" paddingH="-75" paddingV="-20">
			<holdButton out="assets/skins/skin/left.png" over="assets/skins/skin/leftOver.png" down="assets/skins/skin/leftOver.png" action="left" tooltip="" />
			<doubleButton>
				<button out="assets/skins/skin/play.png" over="assets/skins/skin/playOver.png" action="play" tooltip="" />
				<button out="assets/skins/skin/pause.png" over="assets/skins/skin/pauseOver.png" action="pause" tooltip="" />
			</doubleButton>
			<holdButton out="assets/skins/skin/right.png" over="assets/skins/skin/rightOver.png" down="assets/skins/skin/rightOver.png" action="right" tooltip="" />
			
		</group>
		<group snapToCell="true" gridPosition="CR" cellPosition="CR" orientation="horizontal" spacing="2" paddingH="-10" paddingV="-20">
		    <holdButton out="assets/skins/skin/zoomIn.png" over="assets/skins/skin/zoomInOver.png" down="assets/skins/skin/zoomInOver.png" action="zoomIn" tooltip=""/>
